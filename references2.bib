issn = {16713664},
journal = {Earthquake Engineering and Engineering Vibration},
keywords = {Dubai, UAE seismicity,concrete,dual system,special moment-resisting frames (SMRF),special shear wall,steel},
number = {3},
pages = {385--397},
title = {{Impact of lateral force-resisting system and design/construction practices on seismic performance and cost of tall buildings in Dubai, UAE}},
volume = {12},
year = {2013}
}
