journal = {Bulletin of the Seismological Society of America},
month = {feb},
title = {{Accounting for Uncertainties of Magnitude- and Site-Related Parameters on Neural Network-Computed Ground-Motion Prediction Equations}},
url = {https://pubs.geoscienceworld.org/ssa/bssa/article/581032/Accounting-for-Uncertainties-of-Magnitude-and},
year = {2020}
}
