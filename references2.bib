issn = {1369-7412},
journal = {Journal of the Royal Statistical Society: Series B (Statistical Methodology)},
keywords = {EM algorithm,Non-convex penalty,Oracle estimators,Penalized likelihood,Probability model,Variable selection},
month = {aug},
