file = {:Users/nico/BIBLIOGRAPHY/PAPER/Markhvida/Markhvida{\_}et{\_}al-2018.pdf:pdf},
isbn = {6507252573},
issn = {10969845},
journal = {Earthquake Engineering and Structural Dynamics},
keywords = {cross-correlation,principal component analysis,seismic risk,spatial correlation,spectral accelerations},
number = {5},
pages = {1107--1123},
pmid = {25246403},
title = {{Modeling spatially correlated spectral accelerations at multiple periods using principal component analysis and geostatistics}},
volume = {47},
