pages = {105834},
publisher = {Elsevier Ltd},
title = {{Generalised parametric functions and spatial correlations for seismic velocities in the Canterbury, New Zealand region from surface-wave-based site characterisation}},
url = {https://doi.org/10.1016/j.soildyn.2019.105834 https://linkinghub.elsevier.com/retrieve/pii/S0267726118313903},
volume = {128},
