title = {{Ground-motion intensity and damage map selection for probabilistic infrastructure network risk assessment using optimization}},
url = {http://www.stanford.edu/{~}bakerjw/Publications/Baker (2007) Record scaling, 8PCEE.pdf{\%}5Cnpapers2://publication/uuid/1498A307-A88B-4AAF-B3F7-E6B4B14F87C2 http://doi.wiley.com/10.1002/eqe.2506},
