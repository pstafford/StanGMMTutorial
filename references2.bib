@article{Satake2003,
abstract = {The 1700 Cascadia earthquake attained moment magnitude 9 according to new estimates based on effects of its tsunami in Japan, computed coseismic seafloor deformation for hypothetical ruptures in Cascadia, and tsunami modeling in the Pacific Ocean. Reports of damage and flooding show that the 1700 Casscadia tsunami reached 1-5 m heights at seven shoreline sites in Japan. Three sets of estimated heights express uncertainty about location and depth of reported flooding, landward decline in tsunami heights from shorelines, and post-1700 land-level changes. We compare each set with tsunami heights computed from six Cascadia sources. Each source is vertical seafloor displacement calculated with a three-dimensional elastic dislocation model, for three sources the rupture extends the 1100 km length of the subduction zone and differs in width and shallow dip; for the other sources, ruptures of ordinary width extend 360-670 km. To compute tsunami waveforms, we use a linear long-wave approximation with a finite difference method, and we employ modern bathymetry with nearshore grid spacing as small as 0.4 km. The various combinations of Japanese tsunami heights and Cascadia sources give seismic moment of 1-9 × 10 22 N m, equivalent to moment magnitude 8.7-9.2. This range excludes several unquantified uncertainties. The most likely earthquake, of moment magnitude 9.0, has 19 m of coseismic slip on an offshore, full-slip zone 1100 km long with linearly decreasing slip on a downdip partial-slip zone. The shorter rupture models require up to 40 m offshore slip and predict land-level changes inconsistent with coastal paleoseismological evidence. Copyright 2003 by the American Geophysical Union.},
author = {Satake, Kenji and Wang, Kelin and Atwater, Brian F.},
doi = {10.1029/2003JB002521},
file = {:Users/nico/BIBLIOGRAPHY/PAPER/Satake/Satake{\_}et{\_}al-2003.pdf:pdf},
issn = {0148-0227},
journal = {Journal of Geophysical Research},
keywords = {doi:10.1029/2003JB002521,http://dx.doi.org/10.1029/2003JB002521},
number = {B11},
pages = {2535},
title = {{Fault slip and seismic moment of the 1700 Cascadia earthquake inferred from Japanese tsunami descriptions}},
url = {http://doi.wiley.com/10.1029/2003JB002521},
volume = {108},
year = {2003}
}
@inproceedings{Kuehn2011a,
abstract = {The main goal of probabilistic seismic hazard analysis is to calculate the probability of exceedance for a range of shaking levels at a particular site. This requires the use of calculating the conditional probability of a ground motion intensity parameter of engineering interest given some earthquake and site related parameters such as magnitude, distance or Vs30. For that purpose, usually empirical regression models are employed, which are referred to as Ground Motion Prediction Equations (GMPEs). Some problems in this context relate to the functional form of the GMPE and the choice of predictor variables that are assumed to affect the ground motion distribution. Here we address these issues by adhering to the multivariate statistical framework Bayesian Networks (BNs). We investigate the use of Bayesian Networks in PSHA in general, and illustrate the applicability and advantages of employing BNs in the ground motion domain. Our departure point is motivated by the desire to learn from data, inother words, we take a data-driven stance and learn a Bayesian Network from a strong motion dataset consisting of several earthquake, site and ground motion parameters. By analyzing and inspecting the resulting BN this approach allows us to assess the actual probabilistic (in)dependencies that hold between these parameters as supported by the data. We also derive Bayesian networks using "classical" GMPEs. The Bayesian networks for the ground motion domain are extended based on a priori domain knowledge: we incorporate other variables, parameters and relate them in a way that we deem appropriate and relevant for PSHA. Thus, we arrive at a full-fledged multivariate statistical model of PSHA in terms of a Bayesian network. Since Bayesian networks provide a principled framework for reasoning (by way of statistical inference) under uncertainty, this offers the possibility to incorporate various sources of uncertainty and variability that prevail in PSHA into a single network. From this network, we can now easily compute e.g. the conditional distribution of the ground motion parameter of interest given different sets of evidence for the earthquake and site parameters. Hence, we can use the PSHA-Bayesian network to easily investigate the PSHA results with regard to uncertainties of different variables that are important for PSHA, and we can examine the influence of several earthquake and site related parameters on the hazard curve. The use of Bayesian networks for PSHA also offers new perspectives for evaluating/ disaggregating the results of a hazard study, since it is possible to compute any marginal or conditional distribution. {\textcopyright} 2011 Taylor  {\&}  Francis Group, London.},
author = {Kuehn, N.M. and Riggelsen, C. and Scherbaum, F.},
booktitle = {Applications of Statistics and Probability in Civil Engineering -Proceedings of the 11th International Conference on Applications of Statistics and Probability in Civil Engineering},
isbn = {9780415669863},
title = {{On the use of Bayesian networks for probabilistic seismic hazard analysis}},
year = {2011}
}
