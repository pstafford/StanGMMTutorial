eprint = {1604.00860},
file = {:Users/nico/BIBLIOGRAPHY/PAPER/Rue/rue{\_}al{\_}2017.pdf:pdf},
issn = {2326-8298},
journal = {Annual Review of Statistics and Its Application},
keywords = {approximate,bayesian inference,gaussian markov random fields,laplace approximations,latent gaussian models,numerical integration,sparse},
month = {mar},
number = {1},
pages = {395--421},
title = {{Bayesian Computing with INLA: A Review}},
url = {http://www.annualreviews.org/doi/10.1146/annurev-statistics-060116-054045},
volume = {4},
year = {2017}
}
