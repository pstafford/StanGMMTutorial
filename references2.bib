publisher = {Springer Netherlands},
title = {{An analytical solution for the Bayesian estimation of ground motion from macroseismic intensity data}},
url = {https://doi.org/10.1007/s10518-017-0297-1 http://link.springer.com/10.1007/s10518-017-0297-1},
volume = {16},
