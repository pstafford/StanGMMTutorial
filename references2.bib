journal = {Bulletin of the Seismological Society of America},
month = {oct},
number = {5},
pages = {2073--2087},
title = {{Assessing Predictive Capability of Ground‐Motion Models for Probabilistic Seismic Hazard in Iran}},
url = {https://pubs.geoscienceworld.org/ssa/bssa/article/572188/Assessing-Predictive-Capability-of-GroundMotion https://pubs.geoscienceworld.org/ssa/bssa/article/109/5/2073/572188/Assessing-Predictive-Capability-of-GroundMotion},
