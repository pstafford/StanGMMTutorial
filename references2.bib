@article{Hinzen2013,
abstract = {A Roman mausoleum located in the ancient city of Pinara, southwest$\backslash$nTurkey, shows traces of strong dynamic forces. Considering the$\backslash$nseismotectonic potential of the area, earthquake ground motions are a$\backslash$npotential cause of damage. On the other hand, the mausoleum is located$\backslash$nat the foot of an almost 100-m-high steep cliff imposing rockfall hazard$\backslash$nto the building. We constructed a 3D model of the mausoleum based on 90$\backslash$nmillion points from nine laser scans. From this model the shape and size$\backslash$nof the construction was measured and the displacements of all visible$\backslash$nblock vertices have been derived. Based on the 3D model and examples of$\backslash$nsimilar structures a fully reconstructed virtual discrete-element model$\backslash$nof the mausoleum with 180 individual blocks was constructed. This model$\backslash$nwas exposed to earthquake ground motions and the impact of falling$\backslash$nrocks. The impact velocity of the latter was estimated with a$\backslash$nsite-specific rockfall model. The earthquake ground motion was estimated$\backslash$nfrom local earthquake scenarios and quantified with 3D synthetic$\backslash$nstrong-motion seismograms. In addition, analytic ground-motion signals$\backslash$nwere used to study the general dynamic behavior of the mausoleum and the$\backslash$nvulnerability of its columns. Comparison of the in situ measured block$\backslash$ndisplacements and the damage pattern with the results of the rockfall$\backslash$nand earthquake models clearly favor coseismic displacement as the cause$\backslash$nof the damage. A local earthquake with moment magnitude 6.3 is$\backslash$nsufficient to produce the observed damage.},
author = {Hinzen, Klaus G. and Kehmeier, Helen and Schreiber, Stephan},
doi = {10.1785/0120120245},
file = {:Users/nico/BIBLIOGRAPHY/PAPER/Hinzen/hinzen{\_}al{\_}2013{\_}archeo.pdf:pdf},
isbn = {0037-1106},
issn = {00371106},
journal = {Bulletin of the Seismological Society of America},
number = {2 A},
pages = {1008--1021},
title = {{Quantitative archaeoseismological study of a Roman mausoleum in Pi{\{}dotless{\}}nara (Turkey)-Testing seismogenic and rockfall damage scenarios}},
volume = {103},
year = {2013}
}
@article{Gelman2018,
abstract = {The usual definition of R 2 (variance of the predicted values divided by the variance of the data) has a problem for Bayesian fits, as the numerator can be larger than the denominator. We propose an alternative definition similar to one that has appeared in the survival analysis literature: the variance of the predicted values divided by the variance of predicted values plus the variance of the errors. This summary is computed automatically for linear and generalized linear regression models fit using rstanarm, our R package for fitting Bayesian applied regression models with Stan.},
author = {Gelman, Andrew and Goodrich, Ben and Gabry, Jonah and Ali, Imad},
file = {:Users/nico/BIBLIOGRAPHY/PAPER/Gelman/gelman{\_}al{\_}2018.pdf:pdf},
title = {{R-squared for Bayesian regression models}},
url = {http://www.stat.columbia.edu/{~}gelman/research/unpublished/bayes{\_}R2.pdf},
year = {2018}
}
