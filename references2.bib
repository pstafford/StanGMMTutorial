issn = {0956540X},
journal = {Geophysical Journal International},
keywords = {Dynamics and mechanics of faulting,Earthquake source observations,Lithospheric flexure,Rheology and friction of fault zones,Rheology: crust and lithosphere},
number = {1},
pages = {63--89},
title = {{A reassessment of outer-rise seismicity and its implications for the mechanics of oceanic lithosphere}},
volume = {197},
year = {2014}
}
