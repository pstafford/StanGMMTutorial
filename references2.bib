title = {{Hybrid broadband ground motion simulation validation of small magnitude earthquakes in Canterbury, New Zealand}},
url = {http://journals.sagepub.com/doi/10.1177/8755293019891718},
year = {2020}
}
@article{Saffari2010,
abstract = {Existing attenuation relations in Iran mostly deal with peak ground acceleration and peak ground velocity. However, spectral attenuation is needed not only for peak ground acceleration, but also for the entire range of periods. In this study, a new spectral attenuation for acceleration is presented for Iran. To that end, a database of strong ground motion in Iran consisting of 110 earthquakes and 627 accelerograms was developed. We introduced the attenuation relationship for acceleration response spectra with 5{\%} damping. The two primary factors considered were moment magnitude and source distance. A near field term was also used in the model for better estimation of acceleration spectra in the near field. Non-linear regression analysis adopted from Fukushima and Tanaka (1990) was used to obtain coefficients.},
