journal = {Seismological Research Letters},
month = {jun},
number = {4},
pages = {1457--1467},
title = {{Ground‐Motion Characteristics of the 30 November 2018 Injection‐Induced Earthquake Sequence in Northeast British Columbia, Canada}},
url = {https://pubs.geoscienceworld.org/ssa/srl/article/571681/GroundMotion-Characteristics-of-the-30-November},
