file = {:Users/nico/BIBLIOGRAPHY/PAPER/Miller/Miller{\_}al{\_}2020.pdf:pdf},
issn = {1085-7117},
journal = {Journal of Agricultural, Biological and Environmental Statistics},
keywords = {Basis-penalty smoothing,Generalized additive model,Smoothing,Spatial modelling,Stochastic partial differential equations},
month = {mar},
number = {1},
pages = {1--16},
publisher = {Springer US},
title = {{Understanding the Stochastic Partial Differential Equation Approach to Smoothing}},
