title = {{A Ground-Motion Predictive Model for Iran and Turkey for Horizontal PGA, PGV, and 5$\backslash${\%} Damped Response Spectrum: Investigation of Possible Regional Effects}},
url = {https://pubs.geoscienceworld.org/bssa/article/105/2A/963-980/332693},
