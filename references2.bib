journal = {Geophysical Journal International},
keywords = {1 i n t,considers spatial,correlation of earthquake,correlations of earthquake intensity,earthquake hazards,ims,in previous studies,it has been shown,measures,portfolios of buildings needs,ro d u c,seismic anisotropy,seismic hazard analysis that,seismic risk assessment of,spatial analysis,spatially distributed systems and,statistical seismology,t i o n,that neglecting the spatial},
