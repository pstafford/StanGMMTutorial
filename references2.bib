journal = {Journal of Agricultural, Biological, and Environmental Statistics},
keywords = {Bayesian inference,Gaussian predictive process,Regional climate models,Space-time processes},
month = {dec},
number = {4},
pages = {586--605},
title = {{Comparing and Blending Regional Climate Model Predictions for the American Southwest}},
