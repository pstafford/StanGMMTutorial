@article{Allen2020a,
abstract = {This article explores the implementation of the Natural Resources Canada's Fifth Generation national seismic hazard model as developed for the National Building Code of Canada (NBCC), within the OpenQuake-engine. It also describes the reconciliation of the differences in hazard estimates relative to the published NBCC values, calculated using GSCFRISK. Source and ground-motion input models developed for the GSCFRISK software were translated to the OpenQuake-engine format for the hazard comparison. In order to successfully undertake this process, several adjustments to the OpenQuake code were needed to mimic the behavior of GSCFRISK. This required the development of new functions for earthquake-rupture scaling and ground-motion interpolation. Hazard values estimated using the OpenQuake-engine are generally in good agreement with the 2015 NBCC national-scale hazard values, with differences less than 2{\%}–3{\%} typically achieved. Where larger differences arise, they can be rationalized in terms of differences between the behaviors of the two software engines with respect to earthquake-rupture length uncertainty and maximum ground-motion integration distance.},
author = {Allen, Trevor I and Halchuk, Stephen and Adams, John and Weatherill, Graeme A},
doi = {10.1177/8755293019900779},
file = {:Users/nico/BIBLIOGRAPHY/PAPER/Allen/allen{\_}al{\_}2020a.pdf:pdf},
issn = {8755-2930},
journal = {Earthquake Spectra},
keywords = {3 december 2019,6 june 2019,accepted,building codes,characterization,date received,ground-motion characterization,psha software,seismic hazard,seismic-source},
month = {feb},
pages = {875529301990077},
title = {{Forensic PSHA: Benchmarking Canada's Fifth Generation seismic hazard model using the OpenQuake-engine}},
url = {http://journals.sagepub.com/doi/10.1177/8755293019900779},
year = {2020}
}
