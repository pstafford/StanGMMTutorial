@article{Petersen2014,
abstract = {The national seismic hazard maps for the conterminous United States have been updated to account for new methods, models, and data that have been obtained since the 2008 maps were released (Petersen and others, 2008). The input models are improved from those implemented in 2008 by using new ground motion models that have incorporated about twice as many earthquake strong ground shaking data and by incorporating many additional scientific studies that indicate broader ranges of earthquake source and ground motion models. These time-independent maps are shown for 2-percent and 10-percent probability of exceedance in 50 years for peak horizontal ground acceleration as well as 5-hertz and 1-hertz spectral accelerations with 5-percent damping on a uniform firm rock site condition (760 meters per second shear wave velocity in the upper 30 m, VS30). In this report, the 2014 updated maps are compared with the 2008 version of the maps and indicate changes of plus or minus 20 percent over wide areas, with larger changes locally, caused by the modifications to the seismic source and ground motion inputs.},
author = {Petersen, Mark D and Moschetti, Morgan P and Powers, Peter M and Mueller, Charles S and Haller, Kathleen M and Frankel, Arthur D and Zeng, Yuehua and Rezaeian, Sanaz and Harmsen, Stephen C and Boyd, Oliver S and Field, Ned and Chen, Rui and Rukstales, Kenneth S and Luco, Nicolas and Wheeler, Russell L and Williams, Robert A and Olsen, Anna H},
doi = {10.3133/ofr20141091},
file = {:Users/nico/BIBLIOGRAPHY/REPORTS/USGS{\_}2014{\_}hazardmaps.pdf:pdf},
isbn = {OFR 02-420},
