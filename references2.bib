issn = {0895-0695},
journal = {Seismological Research Letters},
month = {may},
number = {3},
pages = {1542--1555},
title = {{Near-Field Ground Motions from the July 2019 Ridgecrest, California, Earthquake Sequence}},
url = {https://pubs.geoscienceworld.org/ssa/srl/article/91/3/1542/581956/NearField-Ground-Motions-from-the-July-2019},
volume = {91},
year = {2020}
}
