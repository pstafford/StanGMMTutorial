file = {:Users/nico/BIBLIOGRAPHY/PAPER/Gruenthal/Gruenthal{\&}Wahlstroem{\_}2006.pdf:pdf},
isbn = {0921-030X},
issn = {0921-030X},
journal = {Natural Hazards},
keywords = {Cologne/Aachen area,Maximum magnitude,Parameter uncertainty,Seismic hazard,Zonation models},
month = {may},
number = {1-2},
pages = {159--176},
title = {{New Generation of Probabilistic Seismic Hazard Assessment for the Area Cologne/Aachen Considering the Uncertainties of the Input Data}},
