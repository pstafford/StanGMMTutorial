journal = {Bulletin of the Seismological Society of America},
number = {6},
pages = {2039--2070},
title = {{Near-source attenuation of peak horizontal acceleration}},
url = {http://www.bssaonline.org/content/71/6/2039.short},
volume = {71},
year = {1981}
}
