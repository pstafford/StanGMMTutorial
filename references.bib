@article{Strasser2010,
abstract = {Living in Christchurch and working in a community psychiatric team after the 7.1 MM intensity earthquake on 4 September 2010 provided the opportunity to examine the psychiatric literature on earthquakes and make some naturalistic observations about anxiety and earthquakes, with a particular focus on the impact of aftershocks. Literature search of articles were varied in nature but mostly commented on service delivery and the mental health consequences of a natural disaster. The period after an earthquake, when aftershocks were occurring, was associated with a further increase in blood pressure in a group of elderly outpatients with hypertension. There is additional literature examining the effects of earthquakes on coronary artery disease with an acute increase in coronary syndromes occurring on the day of major earthquakes, as well as in the years subsequently. The literature on earthquakes and aftershocks suggests that these events cause significant short and long-term mental and physical problems. This is consistent with observations in the Christchurch setting where the initial quake appeared to have a triggering effect and the uncertainty about aftershocks exacerbated frayed nerves and may have longer-term impacts on well-being. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
author = {Strasser, F. O. and Arango, M. C. and Bommer, J. J.},
