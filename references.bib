issn = {1573157X},
journal = {Journal of Seismology},
keywords = {Bracketed duration,Distance,Intraplate earthquakes,Magnitude,Predictive relationship,Significant duration},
number = {4},
pages = {1001--1021},
publisher = {Journal of Seismology},
title = {{Empirical models for the prediction of ground motion duration for intraplate earthquakes}},
volume = {21},
year = {2017}
}
