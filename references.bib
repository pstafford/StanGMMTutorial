@inproceedings{Scherbaum2011c,
abstract = {Logic trees are currrently the most popular tool to quantify epistemic uncertainties in seismic hazard analysis. The assumption in this context, that the candidate models form a partition of a finite categorical model set, however, poses severe limitations in dealing with model redundancy. Here we demonstrate how these limitations can be overcome by representing models (here ground-motion models) as high-dimensional model feature vectors that can be thought of as spanning a two-dimensional model manifold in a high-dimensional space. The structure of this manifold can be analysed through a combination of SOM and Sammon's mapping. Using an information-theoretic distance metric for the generation of the SOMs results in information-based proximity maps that then represent epistemic uncertainty on the underlying models on a continuous interval scale. Here individual models can be seen as samples of a continuous model domain on which model redundancy is no longer an issue. {\textcopyright} 2011 Taylor  {\&}  Francis Group, London.},
author = {Scherbaum, F. and Kuehn, N. and Ohrnberger, M. and Riggelsen, C. and Gianniotis, N.},
booktitle = {Applications of Statistics and Probability in Civil Engineering -Proceedings of the 11th International Conference on Applications of Statistics and Probability in Civil Engineering},
isbn = {9780415669863},
title = {{Quantification of epistemic uncertainties in probabilistic seismic hazard analysis}},
year = {2011}
}
