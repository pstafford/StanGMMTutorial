journal = {Bulletin of the Seismological Society of America},
number = {1B},
pages = {318--330},
title = {{Probabilistic Seismic-Hazard Estimation for Peninsular India}},
url = {http://www.bssaonline.org/cgi/content/abstract/97/1B/318},
volume = {97},
year = {2007}
}
