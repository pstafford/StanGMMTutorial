@article{Hauksson2006,
abstract = {We analyze high dynamic range waveform spectra to determine t* values for both P and S waves from earthquakes in southern California. We invert the t* values for three-dimensional (3-D) frequency-independent Q$\backslash$n$\backslash$nP$\backslash$nand Q$\backslash$n$\backslash$nS$\backslash$nregional models of the crust. The models have 15 km horizontal grid spacing and an average vertical grid spacing of 4 km, down to 22 km depth, and extend from the U.S.-Mexico border to the Coast Ranges in the south and Sierra Nevada in the north. In general, Q$\backslash$n$\backslash$nP$\backslash$nand Q$\backslash$n$\backslash$nS$\backslash$nincrease rapidly with depth, consistent with crustal densities and velocities. The 3-D Q$\backslash$n$\backslash$nP$\backslash$nand Q$\backslash$n$\backslash$nS$\backslash$nmodels image the major tectonic structures and to a much lesser extent the thermal structure of the southern California crust. The near-surface low Q$\backslash$n$\backslash$nP$\backslash$nand Q$\backslash$n$\backslash$nS$\backslash$nzones coincide with major sedimentary basins such as the San Bernardino, Chino, San Gabriel Valley, Los Angeles, Ventura, and Santa Maria basins and the Salton Trough. In contrast, at shallow depths beneath the Peninsular Ranges, southern Mojave Desert, and southern Sierras, we image high Q$\backslash$n$\backslash$nP$\backslash$nand Q$\backslash$n$\backslash$nS$\backslash$nzones, which correspond to the dense and high-velocity rocks of the mountain ranges. Several clear transition zones of rapidly varying Q$\backslash$n$\backslash$nP$\backslash$nand Q$\backslash$n$\backslash$nS$\backslash$ncoincide with major late Quaternary faults and connect regions of high and low Q$\backslash$n$\backslash$nP$\backslash$nand Q$\backslash$n$\backslash$nS$\backslash$n. At midcrustal depths, the Q$\backslash$n$\backslash$nP$\backslash$nand Q$\backslash$n$\backslash$nS$\backslash$nexhibit modest variation in slightly higher and lower Q$\backslash$n$\backslash$nP$\backslash$nor Q$\backslash$n$\backslash$nS$\backslash$nzones, which is consistent with reported crustal reflectivity. In general, for the southern California crust, Q$\backslash$n$\backslash$nS$\backslash$n/Q$\backslash$n$\backslash$nP$\backslash$nis greater than 1.0, suggesting partially fluid-saturated crust. A few limited regions of Q$\backslash$n$\backslash$nS$\backslash$n/Q$\backslash$n$\backslash$nP$\backslash$nless than 1.0 correspond to areas mostly outside the major sedimentary basins, including areas around the San Jacinto fault, suggesting a larger reduction in the shear modulus compared to the bulk modulus or almost complete fluid saturation.},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Hauksson, Egill and Shearer, Peter M.},
doi = {10.1029/2005JB003947},
eprint = {arXiv:1011.1669v3},
file = {:Users/nico/BIBLIOGRAPHY/PAPER/Hauksson/hauksson{\_}Q{\_}jgr06.pdf:pdf},
isbn = {0148-0227},
issn = {01480227},
journal = {Journal of Geophysical Research: Solid Earth},
month = {may},
number = {B5},
pages = {n/a--n/a},
